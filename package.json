    "fastify-graceful-shutdown": "^4.0.1",
    "fastify-plugin": "^4.5.1",
    "node-sql-parser": "^5.2.0",
    "ohash": "^1.1.3",
    "pg": "^8.12.0",
    "pino": "^9.3.1",
    "postgres": "^3.4.4",
    "proj4": "^2.11.0",
    "radash": "^12.1.0",
    "sharp": "^0.33.4",
    "slonik": "^45.6.0",
    "tarn": "^3.0.2",
    "ts-pattern": "^5.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/better-sqlite3": "^7.6.11",
    "@types/geojson": "^7946.0.14",
    "@types/mapbox__shelf-pack": "^3.0.6",
    "@types/mapbox__sphericalmercator": "^1.2.3",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.6",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "pino-pretty": "^11.2.1",
    "tsc-files": "^1.1.4",
    "tsx": "^4.7.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "**/*.ts": [
      "tsc-files --noEmit",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
